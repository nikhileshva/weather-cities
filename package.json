    "debug": "~4.1.1",
    "dotenv": "^7.0.0",
    "express": "~4.16.4",
    "fs": "0.0.1-security",
    "joi": "^14.3.1",
    "lodash": "^4.17.21",
    "mocha": "^6.0.2",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "path": "^0.12.7",
    "pg": "^7.9.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "response-time": "^2.3.2",
